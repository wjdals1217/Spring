package sub1;

public class LgTV {
	
	private Speaker spk; // 초기화가 없기 때문에 null참조 에러가 난다.
	
	public void powerOn() {
		System.out.println("LgTv powerOn...");
	}

	public void powerOff() {
		System.out.println("LgTv powerOff...");
	}
	
	public void soundUp() {
		spk.soundUp();
	}
	
	public void soundDown() {
		spk.soundDown();
	}
}
